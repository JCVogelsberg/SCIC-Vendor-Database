<%= form_for(@vendor) do |f| %>
  <div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name %><br><br>

    <%= f.label :phone %><br>
    <%= f.text_field :phone %><br><br>

    <%= f.label :location %><br>
    <%= f.text_area :location %><br><br>

    <%= f.label :description %><br>
    <%= f.text_area :description %><br><br>
  </div><br>

  <%= f.submit(:class => "btn btn-primary") %>

<% end %>
